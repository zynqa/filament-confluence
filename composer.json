{
  "name": "zynqa/filament-confluence",
  "description": "Confluence integration for FilamentPHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zynqa",
      "email": "info@zynqa.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^3.2",
    "spatie/laravel-settings": "^3.0",
    "guzzlehttp/guzzle": "^7.8",
    "calebporzio/sushi": "^2.5",
    "spatie/laravel-package-tools": "^1.16"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Zynqa\\FilamentConfluence\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zynqa\\FilamentConfluence\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zynqa\\FilamentConfluence\\FilamentConfluenceServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
